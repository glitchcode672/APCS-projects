import java.util.Scanner;
class Lesson_13_Activity_One
{
    public static void main(String[] args) 
    {
      Scanner reader = new Scanner(System.in);
      
      int num = reader.nextInt();
      
      if(num >= 0)
        System.out.println("Positive");
      else
        System.out.println("Negative");
    }
}
